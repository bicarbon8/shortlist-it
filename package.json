{
  "name": "shortlist-it",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf ./docs/*",
    "build": "npm run clean && webpack -o ./docs/ --mode production",
    "build:dev": "npm run clean && webpack --mode development",
    "build:start": "npm run build:dev && cd dist && PORT=4200 npx serve",
    "start": "ASSET_PATH=http://localhost:4200/ webpack serve --open --mode development",
    "start:live": "ASSET_PATH=http://localhost:4200/ webpack serve --open --mode development --live-reload --hot"
  },
  "license": "MIT",
  "author": {
    "name": "Jason Holt Smith",
    "email": "bicarbon8@gmail.com"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.18.6",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.0",
    "typescript": "^4.5.2",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "bootstrap": "^5.2.2",
    "react": "^17.0.2",
    "react-bootstrap": "^2.5.0",
    "react-bootstrap-icons": "^1.9.1",
    "react-dom": "^17.0.2"
  }
}
